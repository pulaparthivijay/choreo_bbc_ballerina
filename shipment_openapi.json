{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Shipment",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "{server}:{port}/shipment",
    "variables" : {
      "server" : {
        "default" : "http://localhost"
      },
      "port" : {
        "default" : "8080"
      }
    }
  } ],
  "paths" : {
    "/getPurCarCards" : {
      "post" : {
        "operationId" : "postGetpurcarcards",
        "parameters" : [ {
          "name" : "id",
          "in" : "header",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "pageNo",
          "in" : "query",
          "required" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int64"
          }
        }, {
          "name" : "pageSize",
          "in" : "query",
          "required" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int64"
          }
        } ],
        "responses" : {
          "201" : {
            "description" : "Created",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ApprCardsRes"
                }
              }
            }
          },
          "500" : {
            "description" : "Internal server error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorPayload"
                }
              }
            }
          }
        }
      }
    },
    "/getSoldCarCards" : {
      "post" : {
        "operationId" : "postGetsoldcarcards",
        "parameters" : [ {
          "name" : "id",
          "in" : "header",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "pageNo",
          "in" : "query",
          "required" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int64"
          }
        }, {
          "name" : "pageSize",
          "in" : "query",
          "required" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int64"
          }
        } ],
        "responses" : {
          "201" : {
            "description" : "Created",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ApprCardsRes"
                }
              }
            }
          },
          "500" : {
            "description" : "Internal server error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorPayload"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "ApprCardsRes" : {
        "required" : [ "cards", "code", "message", "status", "totalPages", "totalRecords" ],
        "type" : "object",
        "properties" : {
          "cards" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Appraisal"
            }
          },
          "code" : {
            "type" : "integer",
            "format" : "int64"
          },
          "message" : {
            "type" : "string"
          },
          "status" : {
            "type" : "boolean"
          },
          "totalRecords" : {
            "type" : "integer",
            "format" : "int64"
          },
          "totalPages" : {
            "type" : "integer",
            "format" : "int64"
          }
        }
      },
      "ErrorPayload" : {
        "type" : "object",
        "properties" : {
          "reason" : {
            "type" : "string",
            "description" : "Reason phrase"
          },
          "path" : {
            "type" : "string",
            "description" : "Request path"
          },
          "method" : {
            "type" : "string",
            "description" : "Method type of the request"
          },
          "message" : {
            "type" : "string",
            "description" : "Error message"
          },
          "timestamp" : {
            "type" : "string",
            "description" : "Timestamp of the error"
          },
          "status" : {
            "type" : "integer",
            "description" : "Relevant HTTP status code",
            "format" : "int32"
          }
        }
      },
      "Utc" : {
        "type" : "array",
        "items" : {
          "oneOf" : [ {
            "type" : "integer",
            "format" : "int64"
          }, {
            "type" : "number",
            "format" : "double"
          } ]
        }
      },
      "Appraisal" : {
        "required" : [ "appraisedValue", "engineType", "transmissionType", "vehicleMake", "vehicleMileage", "vehicleModel", "vehiclePic1", "vehiclePic2", "vehiclePic3", "vehiclePic4", "vehicleSeries", "vehicleYear", "vinNumber" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "int64"
          },
          "clientFirstName" : {
            "type" : "string"
          },
          "clientLastName" : {
            "type" : "string"
          },
          "clientPhNum" : {
            "type" : "string"
          },
          "vinNumber" : {
            "type" : "string"
          },
          "vehicleYear" : {
            "type" : "integer",
            "format" : "int64"
          },
          "vehicleMake" : {
            "type" : "string"
          },
          "vehicleModel" : {
            "type" : "string"
          },
          "vehicleSeries" : {
            "type" : "string"
          },
          "vehicleInterior" : {
            "type" : "integer",
            "format" : "int64"
          },
          "vehicleExtColor" : {
            "type" : "integer",
            "format" : "int64"
          },
          "user_id" : {
            "type" : "string"
          },
          "is_active" : {
            "type" : "boolean"
          },
          "vehiclePic1" : {
            "type" : "string",
            "nullable" : true
          },
          "vehiclePic2" : {
            "type" : "string",
            "nullable" : true
          },
          "vehiclePic3" : {
            "type" : "string",
            "nullable" : true
          },
          "vehiclePic4" : {
            "type" : "string",
            "nullable" : true
          },
          "invntrySts" : {
            "type" : "string"
          },
          "soldOut" : {
            "type" : "boolean"
          },
          "buyerUser_id" : {
            "type" : "string"
          },
          "appraisedValue" : {
            "type" : "number",
            "format" : "float"
          },
          "createdBy" : {
            "type" : "string"
          },
          "createdOn" : {
            "$ref" : "#/components/schemas/Utc"
          },
          "engineType" : {
            "type" : "string"
          },
          "vehicleMileage" : {
            "type" : "integer",
            "format" : "int64"
          },
          "transmissionType" : {
            "type" : "string"
          },
          "invMovedOn" : {
            "$ref" : "#/components/schemas/Utc"
          }
        }
      }
    }
  }
}